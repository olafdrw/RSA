import time
import math

print "Input public key"
pk = input()
print "Input private key"
pvk = input()
print "Input text"
t = raw_input()

output = []
for list1 in t:
    number = ord(list1) - 96
    output.append(number)

a = ''.join(str(e)for e in output)
print "a = ", a

c = int(a)**pk

m = c%pvk
print "m = ", m
