import time
import math

print "Input public key"
pk = input()
print "Input private key"
pvk = input()
print "Input text"
t = raw_input()

output = []
for list1 in t:
    number = ord(list1) - 96
    output.append(number)

a = ''.join(str(e)for e in output)
print "a = ", a

c = int(a)**pk
#print "c = ", c

m = c%pvk
print "m = ", m

output2 = []
for text2 in m:
    text = chr(text2) - 96
    output2.append(text)

b = ''.join(str(e)for e in output2)
print "b = ", b
